    <Routine name="prep_code">
      <CodeComponent name="startcode">
        <Param name="Begin Experiment" updates="constant" val="import random, xlrd&amp;#10;&amp;#10;# randomize seed&amp;#10;random.seed()&amp;#10;&amp;#10;#stimulus file&amp;#10;&amp;#10;infile = 'stimuli_list.xlsx'&amp;#10;&amp;#10;# Within stimuli list 1st 12 faces are black female&amp;#10;# 13 through 52 are white female&amp;#10;# 53 through 64 are black male&amp;#10;# 65 through end (104) are white male&amp;#10;&amp;#10;# first 104 names are female, remaining are male&amp;#10;&amp;#10;# number of trial (total and types)&amp;#10;numtrial = 104&amp;#10;numstudy = 52&amp;#10;numnovel = 52&amp;#10;&amp;#10;numnames = 208&amp;#10;numfemalenames = 104&amp;#10;nummalenames = 104&amp;#10;&amp;#10;numfaces = 104&amp;#10;numfemalefaces = 52&amp;#10;numbffaces = 12&amp;#10;numwffaces = 40&amp;#10;nummalefaces = 52&amp;#10;numbmfaces = 12&amp;#10;numwmfaces = 40&amp;#10;&amp;#10;numstudyfemale = 26&amp;#10;numstudymale = 26&amp;#10;numnovelfemale = 26&amp;#10;numnovelmale = 26&amp;#10;&amp;#10;numstudybf = 6&amp;#10;numstudybm = 6&amp;#10;numstudywf = 20&amp;#10;numstudywm = 20&amp;#10;numnovelbf = 6 &amp;#10;numnovelbm = 6&amp;#10;numnovelwf = 20&amp;#10;numnovelwm = 20&amp;#10;" valType="extendedCode"/>
        <Param name="Begin JS Experiment" updates="constant" val="insheet = [];&amp;#10;function successFunction(data) {&amp;#10;    var allRows = data.split('\n'); // split rows at new line&amp;#10;    var headerRows = allRows[0].split(',');&amp;#10;    &amp;#10;    for (var i=1; i&lt;allRows.length; i++) {&amp;#10;        var obj = {};&amp;#10;        var currentLine = allRows[i].split(',');&amp;#10;        for(var j=0;j&lt;headerRows.length;j++){&amp;#10;              obj[headerRows[j]] = currentLine[j];&amp;#10;        }&amp;#10;        insheet.push(obj);&amp;#10;    }&amp;#10;}&amp;#10;&amp;#10;$.ajax({&amp;#10;  url: 'resources/stimuli_list.csv',&amp;#10;  dataType: 'text',&amp;#10;  async: false,&amp;#10;}).done(successFunction);&amp;#10;numtrial = 78;&amp;#10;numstudy = 78;&amp;#10;numnovel = 0;&amp;#10;numnames = 156;&amp;#10;numfemalenames = 78;&amp;#10;nummalenames = 78;&amp;#10;numfaces = 78;&amp;#10;numfemalefaces = 39;&amp;#10;numbffaces = 9;&amp;#10;numwffaces = 30;&amp;#10;nummalefaces = 39;&amp;#10;numbmfaces = 9;&amp;#10;numwmfaces = 30;&amp;#10;numstudyfemale = 39;&amp;#10;numstudymale = 39;&amp;#10;numnovelfemale = 0;&amp;#10;numnovelmale = 0;&amp;#10;numstudybf = 9;&amp;#10;numstudybm = 9;&amp;#10;numstudywf = 30;&amp;#10;numstudywm = 30;&amp;#10;numnovelbf = 0;&amp;#10;numnovelbm = 0;&amp;#10;numnovelwf = 0;&amp;#10;numnovelwm = 0;&amp;#10;shuffle = function (a) {&amp;#10;    var j, x, i;&amp;#10;    for (i = a.length - 1; i &gt; 0; i--) {&amp;#10;        j = Math.floor(Math.random() * (i + 1));&amp;#10;        x = a[i];&amp;#10;        a[i] = a[j];&amp;#10;        a[j] = x;&amp;#10;        }&amp;#10;    return a;&amp;#10;    }&amp;#10;" valType="extendedCode"/>
        <Param name="Begin JS Routine" updates="constant" val="bfemalefaces = [];&amp;#10;wfemalefaces = [];&amp;#10;femalenames = [];&amp;#10;bmalefaces = [];&amp;#10;wmalefaces = [];&amp;#10;malenames = [];&amp;#10;row = [];&amp;#10;for (var rowx = 0, _pj_a = numtrial; (rowx &lt; _pj_a); rowx += 1) {&amp;#10;    row = insheet[rowx];&amp;#10;    const {Filepaths: face} = row; &amp;#10;    if ((rowx &lt; numbffaces)) {&amp;#10;        bfemalefaces.push(face);&amp;#10;    } else {&amp;#10;        if (((numbffaces &lt;= rowx) &amp;&amp; (rowx &lt; numfemalefaces))) {&amp;#10;            wfemalefaces.push(face);&amp;#10;        } else {&amp;#10;            if (((numfemalefaces &lt;= rowx) &amp;&amp; (rowx &lt; (numfemalefaces + numbmfaces)))) {&amp;#10;                bmalefaces.push(face);&amp;#10;            } else {&amp;#10;                if ((((numfemalefaces + numbmfaces) &lt;= rowx) &amp;&amp; (rowx &lt; numfaces))) {&amp;#10;                    wmalefaces.push(face);&amp;#10;                }&amp;#10;            }&amp;#10;        }&amp;#10;    }&amp;#10;}&amp;#10;&amp;#10;row = [];&amp;#10;for (var rowx = 0, _pj_a = numnames; (rowx &lt; _pj_a); rowx += 1) {&amp;#10;    row = insheet[rowx];&amp;#10;    const {Names: name} =  row;&amp;#10;    if ((rowx &lt; numfemalenames)) {&amp;#10;        femalenames.push(name);&amp;#10;    } else {&amp;#10;        if ((rowx &gt;= numfemalenames)) {&amp;#10;            malenames.push(name);&amp;#10;        }&amp;#10;    }&amp;#10;}&amp;#10;shuffle(bfemalefaces);&amp;#10;shuffle(wfemalefaces);&amp;#10;shuffle(bmalefaces);&amp;#10;shuffle(wmalefaces);&amp;#10;shuffle(femalenames);&amp;#10;shuffle(malenames);&amp;#10;&amp;#10;face_files = [];&amp;#10;fullnames = [];&amp;#10;old_new = [];&amp;#10;novelnames = [];&amp;#10;i = 0;&amp;#10;g = 0;&amp;#10;h = 0;&amp;#10;j = 0;&amp;#10;k = 0;&amp;#10;l = 0;&amp;#10;m = 0;&amp;#10;for (var i = 0, _pj_a = numnames; (i &lt; _pj_a); i += 1) {&amp;#10;    if ((i &lt; numstudybf)) {&amp;#10;        face_files.push(bfemalefaces[j]);&amp;#10;        fullnames.push(femalenames[g]);&amp;#10;        g = (g + 1);&amp;#10;        j = (j + 1);&amp;#10;    } else {&amp;#10;        if (((numstudybf &lt;= i) &amp;&amp; (i &lt; numstudyfemale))) {&amp;#10;            face_files.push(wfemalefaces[l]);&amp;#10;            fullnames.push(femalenames[g]);&amp;#10;            g = (g + 1);&amp;#10;            l = (l + 1);&amp;#10;        } else {&amp;#10;            if (((numstudyfemale &lt;= i) &amp;&amp; (i &lt; (numstudyfemale + numstudybm)))) {&amp;#10;                face_files.push(bmalefaces[k]);&amp;#10;                fullnames.push(malenames[h]);&amp;#10;                k = (k + 1);&amp;#10;                h = (h + 1);&amp;#10;            } else {&amp;#10;                if ((((numstudyfemale + numstudybm) &lt;= i) &amp;&amp; (i &lt; numstudy))) {&amp;#10;                    face_files.push(wmalefaces[m]);&amp;#10;                    fullnames.push(malenames[h]);&amp;#10;                    m = (m + 1);&amp;#10;                    h = (h + 1);&amp;#10;                    } else {&amp;#10;                        if (((numtrial &lt;= i) &amp;&amp; (i &lt; (numtrial + numstudyfemale)))) {&amp;#10;                            novelnames.push(femalenames[g]);&amp;#10;                            g = (g + 1);&amp;#10;                            } else {&amp;#10;                                if ((((numtrial + numstudyfemale) &lt;= i) &amp;&amp; (i &lt; (numtrial + numstudy)))) {&amp;#10;                                    novelnames.push(malenames[h]);&amp;#10;                                    h = (h + 1);&amp;#10;                                    }&amp;#10;                                }&amp;#10;                            }&amp;#10;                        }&amp;#10;                    }&amp;#10;                }&amp;#10;            }&amp;#10;&amp;#10;&amp;#10;femalelurenames = [];&amp;#10;malelurenames = [];&amp;#10;for (var i = 0, _pj_a = numstudy; (i &lt; _pj_a); i += 1) {&amp;#10;    if ((i &lt; numstudyfemale)) {&amp;#10;        femalelurenames.push(fullnames[i]);&amp;#10;    } else {&amp;#10;        if (((numstudyfemale &lt;= i) &amp;&amp; (i &lt; numstudy))) {&amp;#10;            malelurenames.push(fullnames[i]);&amp;#10;        }&amp;#10;    }&amp;#10;}&amp;#10;" valType="extendedCode"/>
        <Param name="Begin Routine" updates="constant" val="# access stimulus file&amp;#10;inbook = xlrd.open_workbook(infile)&amp;#10;insheet = inbook.sheet_by_index(0) # look at sheet 1&amp;#10;&amp;#10;# arrays for stimuli&amp;#10;&amp;#10;bfemalefaces = []&amp;#10;wfemalefaces = []&amp;#10;femalenames  = []&amp;#10;bmalefaces   = []&amp;#10;wmalefaces   = []&amp;#10;malenames    = []&amp;#10;&amp;#10;# read the excel stimuli&amp;#10;for rowx in range(numtrial):&amp;#10;    &amp;#10;    #read the values of all columns in this row&amp;#10;    row = insheet.row_values(rowx)&amp;#10;    &amp;#10;    # save face to arrays&amp;#10;    &amp;#10;    if rowx &lt; (numbffaces):&amp;#10;        bfemalefaces.append(row[0])&amp;#10;    elif numbffaces &lt;= rowx &lt; (numfemalefaces):&amp;#10;        wfemalefaces.append (row[0])&amp;#10;    elif numfemalefaces &lt;= rowx &lt; (numfemalefaces + numbmfaces):&amp;#10;        bmalefaces.append (row[0])&amp;#10;    elif (numfemalefaces + numbmfaces) &lt;= rowx &lt; numfaces:&amp;#10;        wmalefaces.append (row[0])&amp;#10;&amp;#10;for rowx in range(numnames):&amp;#10;    &amp;#10;    row = insheet.row_values(rowx)&amp;#10;    &amp;#10;    if rowx &lt; numfemalenames:&amp;#10;        femalenames.append(row[1])&amp;#10;    elif rowx &gt;= numfemalenames:&amp;#10;        malenames.append (row[1])&amp;#10;&amp;#10;# randomize stimuli&amp;#10;random.shuffle(bfemalefaces)&amp;#10;random.shuffle(wfemalefaces)&amp;#10;random.shuffle(bmalefaces)&amp;#10;random.shuffle(wmalefaces)&amp;#10;&amp;#10;random.shuffle(femalenames)&amp;#10;random.shuffle(malenames)&amp;#10;&amp;#10;# final arrays for stimuli&amp;#10;&amp;#10;face_files = []&amp;#10;fullnames  = []&amp;#10;old_new    = []&amp;#10;novelnames = []&amp;#10;&amp;#10;# set some counters&amp;#10;i = 0&amp;#10;g = 0 # female counter&amp;#10;h = 0 # male counter&amp;#10;j = 0 # black female counter&amp;#10;k = 0 # black male counter&amp;#10;l = 0 # white female counter&amp;#10;m = 0 # white male counter&amp;#10;&amp;#10;for i in range(numnames):&amp;#10;    if i &lt; (numstudybf): # if its black female to be studied&amp;#10;       face_files.append(bfemalefaces[j]) # append next black female face&amp;#10;       fullnames.append(femalenames[g]) # append next female name&amp;#10;       g = g + 1&amp;#10;       j = j + 1&amp;#10;    elif numstudybf &lt;= i &lt; (numstudyfemale): # if its white female to be studied&amp;#10;        face_files.append(wfemalefaces[l]) # append next white female face&amp;#10;        fullnames.append(femalenames[g]) # append next female name&amp;#10;        g = g + 1&amp;#10;        l = l + 1&amp;#10;    elif numstudyfemale &lt;= i &lt; (numstudyfemale + numstudybm): # if its black male to be studied&amp;#10;        face_files.append(bmalefaces[k]) # append next black male face&amp;#10;        fullnames.append(malenames[h]) # append next male name&amp;#10;        k = k + 1&amp;#10;        h = h + 1&amp;#10;    elif (numstudyfemale + numstudybm) &lt;= i &lt; numstudy: # if white male to be studied&amp;#10;        face_files.append(wmalefaces[m]) # append next white male face&amp;#10;        fullnames.append(malenames[h]) # append next male name&amp;#10;        m = m + 1&amp;#10;        h = h + 1&amp;#10;    elif numstudy &lt;= i &lt; (numstudy + numnovelbf): # if black female novel&amp;#10;        face_files.append(bfemalefaces[j]) # append next black female face&amp;#10;        fullnames.append(femalenames[g]) # append next female name&amp;#10;        g = g + 1&amp;#10;        j = j + 1&amp;#10;    elif (numstudy + numnovelbf) &lt;= i &lt; (numstudy + numnovelfemale): # if white novel female&amp;#10;        face_files.append(wfemalefaces[l]) # append next white female face&amp;#10;        fullnames.append(femalenames[g]) # append next female name&amp;#10;        g = g + 1&amp;#10;        l = l + 1&amp;#10;    elif (numstudy + numnovelfemale) &lt;= i &lt; (numstudy + numnovelfemale + numnovelbm): # if black novel male&amp;